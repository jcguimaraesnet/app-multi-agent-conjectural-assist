{
  "name": "conjectural-assist-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:agent\" --names front,back,agent --prefix-colors blue,green,yellow --kill-others",
    "dev:backend": "cd backend && uv run python -m uvicorn main:app --reload --port 8000",
    "dev:frontend": "next dev",
    "dev:original": "next dev --webpack",
    "dev:turbo": "next dev --turbopack",
    "dev:agent": "cd backend && npx @langchain/langgraph-cli dev --port 8123 --no-browser",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.51.2",
    "@copilotkit/react-ui": "^1.51.2",
    "@copilotkit/runtime": "^1.51.2",
    "@ag-ui/client": "0.0.43",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
