{
  "name": "conjectural-assist-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:agent\" --names front,back,agent --prefix-colors blue,green,yellow --kill-others",
    "dev:backend": "cd backend && uv run python -m uvicorn main:app --reload --port 8000",
    "dev:frontend": "next dev",
    "dev:original": "next dev --webpack",
    "dev:turbo": "next dev --turbopack",
    "dev:agent": "cd backend && npx @langchain/langgraph-cli dev --port 8123 --no-browser",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@copilotkit/react-core": "1.51.4",
    "@copilotkit/react-ui": "1.51.4",
    "@copilotkit/runtime": "1.51.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.97.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "onborda": "^1.2.5",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tailwind-merge": "^3.5.0"
  },
  "pnpm": {
    "overrides": {
      "@ag-ui/client": "0.0.43"
    },
    "patchedDependencies": {
      "@copilotkit/runtime@1.51.4": "patches/@copilotkit__runtime@1.51.4.patch"
    }
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^20.19.33",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
